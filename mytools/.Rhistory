library(mytools)
library(ggplot2)
# Basic usage
ggplot(mtcars, aes(x = mpg, y = wt)) +
geom_point() +
theme_ds()
# Setting font size and font family
ggplot(mtcars, aes(x = mpg, y = wt)) +
geom_point() +
theme_ds(base_size = 14, base_family = "Arial")
